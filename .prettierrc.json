{
  "printWidth": 120, 
  "semi": true, 
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all", 
  "proseWrap": "never", 
  "htmlWhitespaceSensitivity": "strict", 
  "endOfLine": "auto", 
  "overrides": [
    {
      "files": "*rc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
